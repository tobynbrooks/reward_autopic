{
  "expo": {
    "name": "Tyre Rewards",
    "slug": "tyre-rewards-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#00704A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tyre-rewards-app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#00704A"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
